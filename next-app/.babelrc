{
	"presets": ["next/babel"],
	"plugins": [
		"@babel/plugin-proposal-nullish-coalescing-operator",
		"@babel/plugin-proposal-optional-chaining",
		"@babel/plugin-transform-shorthand-properties",
		[
			"babel-plugin-direct-import",
			{ "modules": ["@mui/material", "@mui/icons-material"] }
		]
	]
}
